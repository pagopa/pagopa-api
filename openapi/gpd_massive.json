          "tags" : [ "File Upload API" ],
          "summary" : "The Organization creates the debt positions listed in the file.",
          "operationId" : "upload-debt-positions-file",
          "parameters" : [ {
            "name" : "broker_code",
            "in" : "path",
            "description" : "The broker code",
            "required" : true,
            "schema" : {
              "minLength" : 1,
              "type" : "string"
            }
          }, {
            "name" : "organization_fiscal_code",
