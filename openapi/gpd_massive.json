          "requestBody" : {
            "content" : {
              "multipart/form-data" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "file" : {
                      "type" : "string",
                      "description" : "File to be uploaded",
                      "format" : "binary"
                    }
                  }
                },
                "encoding" : {
                  "file" : {
                    "contentType" : "application/octet-stream"
                  }
                }
              }
            },
            "required" : true
          },
          "responses" : {
            "202": {
              "description": "Request accepted.",
              "headers" : {
                "Location" : {
                  "schema" : {
                    "type" : "string",
                    "format" : "url",
                    "example" : "brokers/{broker}/organizations/{organization}/debtpositions/file/{file_id}/status"
                  }
                }
              }
            },
            "400" : {
              "description" : "Malformed request.",
              "content" : {
                "application/json" : {
                  "schema" : {
                    "$ref" : "#/components/schemas/ProblemJson"
                  }
                }
              }
            },
            "401" : {
              "description" : "Wrong or missing function key.",
              "content" : {
                "application/json" : {
                  "schema" : {
                    "$ref" : "#/components/schemas/ProblemJson"
                  }
                }
              }
            },
            "409" : {
              "description" : "Conflict: duplicate file found.",
              "content" : {
                "application/json" : {
                  "schema" : {
                    "$ref" : "#/components/schemas/ProblemJson"
                  }
                }
