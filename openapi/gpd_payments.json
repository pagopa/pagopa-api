{
  "openapi": "3.0.1",
  "info": {
    "title": "PagoPA API Payments",
    "description": "Payments",
    "termsOfService": "https://www.pagopa.gov.it/",
    "version": "0.12.50"
  },
  "servers": [
    {
      "url": "https://api.uat.platform.pagopa.it/gpd/payments-receipts-service/v1"
